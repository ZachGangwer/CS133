"use strict";function calculator(a,b){for(var c=a-b,d={quarters:.25,dimes:.1,nickles:.05,pennies:.01};c>=1;)c-=1;for(var e in d){var f=d[e];d[e]=changify(c,f),c-=d[e]*f}return d.id="green",d}function changify(a,b){var c=void 0;return c=a%b>0?Math.floor(a/b):0}function write(a){var b=document.createElement("li"),c=document.getElementById("output");for(var d in a)"id"!=d&&(b.appendChild(document.createTextNode(d+" "+a[d])),b.appendChild(document.createElement("br")));b.appendChild(document.createTextNode("---END---")),b.setAttribute("id",a.id),c.appendChild(b)}window.onload=function(){document.getElementById("start").onclick=function(){var a=document.getElementById("paid").value,b=document.getElementById("cost").value,c=calculator(a,b);write(c)}};